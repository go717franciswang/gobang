{
    "version": "1.5.0-beta",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "out": "main.js"
    },
    "filesGlob": [
        "./**/*.ts",
        "./**/*.tsx",
        "!./node_modules/**/*"
    ],
    "files": [
        "./Boot.ts",
        "./Game.ts",
        "./MainMenu.ts",
        "./Preloader.ts",
        "./SinglePlayer.ts",
        "./gobang.ts",
        "./move.ts",
        "./p2.d.ts",
        "./phaser.d.ts",
        "./pixi.d.ts",
        "./player.ts"
    ]
}
